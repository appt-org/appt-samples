{
  "name": "@appt.org/samples",
  "version": "1.0.0-alpha.3",
  "description": "Core TypeScript library for loading multi-platform code samples",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./code-samples": "./dist/code-samples",
    "./code-samples/*": "./dist/code-samples/*",
    "./dist/code-samples/*": "./dist/code-samples/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "vite build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appt-org/appt-samples.git"
  },
  "bugs": {
    "url": "https://github.com/appt-org/appt-samples/issues"
  },
  "homepage": "https://github.com/appt-org/appt-samples#readme",
  "keywords": [
    "code",
    "sample",
    "typescript",
    "multi-platform"
  ],
  "author": {
    "name": "Appt Foundation",
    "email": "info@appt.org",
    "url": "https://appt.org"
  },
  "contributors": [
    {
      "name": "Lisa Klein"
    },
    {
      "name": "Layla Hoogeveen"
    },
    {
      "name": "Djovanni Tehubijuluw"
    },
    {
      "name": "Jan Jaap de Groot"
    },
    {
      "name": "Mart-Jan Roeleveld"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.3",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "extends": "../package.json"
  }
}
