{
  "name": "@appt-org/samples-core",
  "version": "0.1.0",
  "description": "Core TypeScript library for loading multi-platform code samples",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "prepublishOnly": "npm test && npm run build"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "keywords": [
    "code",
    "sample",
    "typescript",
    "multi-platform"
  ],
  "author": {
    "name": "Appt Foundation",
    "email": "info@appt.org",
    "url": "http://appt.org"
  },
  "contributors": [
    {
      "name": "Lisa Klein"
    },
    {
      "name": "Layla Hoogeveen"
    },
    {
      "name": "Djovanni Tehubijuluw"
    },
    {
      "name": "Jan Jaap de Groot"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "22.14.0"
  }
}
